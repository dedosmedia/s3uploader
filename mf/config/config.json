{
    "aws_key": "",
    "aws_secret": "",
    "bucket": "empgroup-pr",
    "bucket-path": "webm2mp4/input/",
    "region": "us-east-1",
    "watch-extension": "json",
    "s3-acl": "private",
    "monitoring-delay": 5,
    "metadata": {
        "name":"userName",
        "email":"userEmail",
        "age":"userAge",
        "city":"userCity"
    },
    "log-config": {
        "version": 1,
        "root": {
            "level": "DEBUG",
            "handlers": [
                "console",
                "file_all",
                "file_error"
            ]
        },
        "formatters": {
            "simple": {
                "format": "%(asctime)s %(name)-12s %(levelname)-8s %(message)s"
            }
        },
        "handlers": {
            "console": {
                "formatter": "simple",
                "class": "logging.StreamHandler",
                "stream": "ext://sys.stdout",
                "level": "DEBUG"
            },
            "file_all": {
                "backupCount": 7,
                "level": "DEBUG",
                "interval": 1,
                "when": "d",
                "filename": "logs/app_all.log",
                "encoding": "utf8",
                "formatter": "simple",
                "class": "logging.handlers.TimedRotatingFileHandler"
            },
            "file_error": {
                "backupCount": 7,
                "level": "ERROR",
                "interval": 1,
                "when": "d",
                "filename": "logs/app_error.log",
                "encoding": "utf8",
                "formatter": "simple",
                "class": "logging.handlers.TimedRotatingFileHandler"
            }
        }
    }
}